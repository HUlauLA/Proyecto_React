{
  "name": "project_react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:mock:api\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "mock:api": "json-server --watch db.json --port 3001 --host 0.0.0.0",
